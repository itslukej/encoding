    "@babel/plugin-syntax-bigint": "^7.4.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/base64-js": "^1.2.5",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.5",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.4"
